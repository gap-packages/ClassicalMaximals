[0*Z(4),0*Z(4),0*Z(4),0*Z(4),0*Z(4),0*Z(4),0*Z(4),0*Z(4),Z(4)^3],
[Z(4)^1,Z(4)^1,Z(4)^1,0*Z(4),0*Z(4),0*Z(4),0*Z(4),Z(4)^3,Z(4)^3]
